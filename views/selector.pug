extends layout

block content
  h1= title
  - var image_names=["img1.jpeg","img2.jpeg","img3.jpeg","img4.jpeg","img5.jpeg"]
  - for(let i=0;i<3;i++){
     - let random= Math.floor(Math.random()*5)
     img(src=`/images/${image_names[random]}` alt=`Image No ${random}` height="300px" width="300px")

  - } 